{
  "name": "p5-boilerplate",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "A boilerplate project for p5.js with TypeScript and Vite.",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts && npx prettier --check ./src/",
    "prettier": "npx prettier --write ./src/",
    "test": "vitest run"
  },
  "dependencies": {
    "p5": "^1.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/p5": "^1.7.7",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "prettier": "3.7.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^5.2.0",
    "vitest": "^4.0.14"
  }
}
